<?xml version="1.0"?>
<block>
  <name>freq_timing_estimator</name>
  <key>cdma_freq_timing_estimator</key>
  <category>cdma</category>
  <import>import cdma</import>
  <make>cdma.freq_timing_estimator($seq1, $seq2, $factor, $alpha, $freqs)</make>
  <callback>set_seq1($seq1)</callback>
  <callback>set_seq2($seq2)</callback>
  <callback>set_factor($factor)</callback>
  <callback>set_alpha($alpha)</callback>
  <callback>set_freqs($freqs)</callback>

  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->


  <param>
    <name>seq1</name>
    <key>seq1</key>
    <value>(0+0j,)</value>
    <type>raw</type>
  </param>

  <param>
    <name>seq2</name>
    <key>seq2</key>
    <value>(0+0j,)</value>
    <type>raw</type>
  </param>

  <param>
    <name>factor</name>
    <key>factor</key>
    <value>0</value>
    <type>raw</type>
  </param>

  <param>
    <name>alpha</name>
    <key>alpha</key>
    <value>0.01</value>
    <type>raw</type>
  </param>
  <param>
    <name>freqs</name>
    <key>freqs</key>
    <value>9*(0,)</value>
    <type>raw</type>
  </param>
  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>complex</type>
    <vlen>1</vlen>
  </sink>
  <source>
    <name>flag</name>
    <type>byte</type>
    <vlen>1</vlen>
  </source>
  <source>
    <name>out_freq</name>
    <type>float</type>
    <vlen>1</vlen>
  </source>
  <source>
    <name>out_corr</name>
    <type>float</type>
    <vlen>1</vlen>
  </source>
</block>
